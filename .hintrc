{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-viewport": "off",
    "highest-available-document-mode": "off",
    "detect-css-reflows/layout": "off",
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off",
        "button-name": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "detect-css-reflows/composite": "off",
    "detect-css-reflows/paint": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "user-select",
          "text-decoration: blink",
          "backdrop-filter"
        ]
      }
    ],
    "button-type": "off",
    "axe/parsing": "off"
  }
}